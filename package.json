{
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "@material-ui/core": "^3.9.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "color": "^3.1.0",
    "immer": "^2.1.2",
    "lodash": "^4.17.11",
    "modern-normalize": "^0.5.0",
    "next": "8.0.3",
    "nprogress": "^0.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "twemoji": "^11.3.0"
  },
  "scripts": {
    "jp": "APP_LOCALE=jp next -p 3001",
    "en": "APP_LOCALE=en next -p 3000",
    "dev": "yarn prepare && concurrently \"yarn jp\" \"yarn en\" \"yarn contents:watch\"",
    "contents": "node ./scripts/generateContentsBundle.js",
    "contents:watch": "node ./scripts/generateContentsBundle.js watch",
    "prepare": "tsc",
    "build:en": "yarn prepare && APP_LOCALE=en next build && APP_LOCALE=en next export",
    "build:jp": "yarn prepare && APP_LOCALE=jp next build && APP_LOCALE=jp next export",
    "twemoji": "mkdir -p .twemoji && rm -f src/components/Twemoji/* && rm -f .twemoji/* && cp `node ./scripts/copyUsedEmojis.js` .twemoji && svgr --filename-case kebab  --no-dimensions -d src/components/Twemoji .twemoji",
    "type-check": "tsc -w",
    "test": "jest"
  },
  "devDependencies": {
    "@svgr/cli": "^4.1.0",
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/material-ui": "^0.21.6",
    "@types/next": "^8.0.1",
    "@types/nprogress": "^0.0.29",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "chokidar": "^2.1.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "svg-url-loader": "^2.3.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  }
}
