{
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@material-ui/core": "^4.0.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "color": "^3.1.1",
    "lodash": "^4.17.11",
    "luxon": "^1.15.0",
    "modern-normalize": "^0.5.0",
    "next": "8.1.0",
    "nprogress": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "smoothscroll-polyfill": "^0.4.4",
    "twemoji": "^12.0.4"
  },
  "scripts": {
    "jp": "IS_DEVELOPMENT=true DEV_LOCALE=jp next -p 9991",
    "en": "IS_DEVELOPMENT=true DEV_LOCALE=en next -p 9990",
    "jp:hide-cards": "DEV_LOCALE=jp next -p 9991",
    "en:hide-cards": "DEV_LOCALE=en next -p 9990",
    "dev": "rm -rf .next && yarn tsc && yarn eslint && concurrently \"yarn jp\" \"yarn en\" \"yarn contents:watch\" \"yarn twemoji:watch\"",
    "dev:hide-cards": "rm -rf .next && yarn tsc && yarn eslint && concurrently \"yarn jp:hide-cards\" \"yarn en:hide-cards\" \"yarn contents:watch\" \"yarn twemoji:watch\"",
    "contents": "node ./scripts/generateContentsBundle.js",
    "contents:watch": "node ./scripts/generateContentsBundle.js watch",
    "tsc": "tsc",
    "eslint": "eslint --ext .js,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.ts,.tsx --fix .",
    "build:en": "yarn tsc && yarn eslint && PRODUCTION_LOCALE=en next build && PRODUCTION_LOCALE=en next export",
    "build:jp": "yarn tsc && yarn eslint && PRODUCTION_LOCALE=jp next build && PRODUCTION_LOCALE=jp next export",
    "twemoji": "mkdir -p .twemoji && rm -f src/components/Twemoji/* && rm -f .twemoji/* && cp `node ./scripts/copyUsedEmojis.js` .twemoji && svgr --no-svgo --filename-case kebab --no-dimensions -d src/components/Twemoji .twemoji && yarn twemoji:bundle",
    "twemoji:bundle": "node ./scripts/generateEmojisBundle.js",
    "twemoji:watch": "node ./scripts/generateEmojisBundle.js watch",
    "svg": "rm -rf src/components/Svg && svgr -d src/components/Svg static/images/svg",
    "type-check": "tsc -w",
    "test": "jest"
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.0",
    "@types/color": "^3.0.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.132",
    "@types/luxon": "^1.15.1",
    "@types/material-ui": "^0.21.6",
    "@types/next": "^8.0.5",
    "@types/nprogress": "^0.0.29",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-emotion": "^10.0.9",
    "chokidar": "^3.0.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "glob": "^7.1.4",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  }
}
