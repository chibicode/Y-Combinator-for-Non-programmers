{
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/pro-light-svg-icons": "^5.3.1",
    "@fortawesome/pro-regular-svg-icons": "^5.3.1",
    "@fortawesome/pro-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "color": "^3.0.0",
    "emotion": "^9.2.10",
    "emotion-server": "^9.2.10",
    "emotion-theming": "^9.2.9",
    "immer": "^1.7.2",
    "lodash": "^4.17.11",
    "modern-normalize": "^0.5.0",
    "next": "7.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "twemoji": "^11.0.1"
  },
  "scripts": {
    "jp": "APP_LOCALE=jp next -p 3001",
    "en": "APP_LOCALE=en next -p 3000",
    "dev": "yarn prepare && concurrently \"yarn jp\" \"yarn en\" \"yarn contents:watch\"",
    "contents": "node ./scripts/generateContentsBundle.js",
    "contents:watch": "node ./scripts/generateContentsBundle.js watch",
    "prepare": "tsc && yarn contents && yarn twemoji",
    "build:en": "yarn prepare && APP_LOCALE=en next build && APP_LOCALE=en next export",
    "build:jp": "yarn prepare && APP_LOCALE=jp next build && APP_LOCALE=jp next export",
    "twemoji": "mkdir -p .twemoji && rm -f src/components/Twemoji/* && rm -f .twemoji/* && cp `node ./scripts/copyUsedEmojis.js` .twemoji && svgr --filename-case kebab  --no-dimensions -d src/components/Twemoji .twemoji",
    "type-check": "tsc -w",
    "test": "jest"
  },
  "devDependencies": {
    "@svgr/cli": "^3.0.0",
    "@types/color": "^3.0.0",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/material-ui": "^0.21.5",
    "@types/next": "^7.0.0",
    "@types/nprogress": "^0.0.29",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "babel-plugin-emotion": "^9.2.10",
    "chokidar": "^2.0.4",
    "concurrently": "^4.0.1",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "prettier-tslint": "^0.4.0",
    "ts-jest": "^23.10.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  }
}
